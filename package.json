    "@testing-library/react": "^10.4.3",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.41",
    "@types/react-datepicker": "^3.0.2",
