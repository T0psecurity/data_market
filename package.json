    "gatsby-transformer-remark": "^2.8.20",
    "is-url-superb": "^4.0.0",
    "numeral": "^2.0.6",
    "react": "^16.13.1",
    "react-data-table-component": "^6.9.3",
    "react-datepicker": "^3.0.0",
    "react-dom": "^16.13.1",
